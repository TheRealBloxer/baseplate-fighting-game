{
  "name": "baseplate-fighting-game",
  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
      "$path": "src/ReplicatedStorage",
      
      "Tools": {
        "$className": "Folder"
      },

      "Events": {
        "$className": "Folder",

        "PlayerLeavingLobby": {
          "$className": "RemoteEvent"
        },

        "PlayerJoinedGame": {
          "$className": "RemoteEvent"
        },

        "ToolEquipped": {
          "$className": "RemoteEvent"
        },

        "VoteForFreeForAll": {
          "$className": "RemoteEvent"
        },

        "VoteForTeamDeathmatch": {
          "$className": "RemoteEvent"
        },

        "VotingChanged": {
          "$className": "RemoteEvent"
        },

        "VotingBegan": {
          "$className": "RemoteEvent"
        },

        "VotingEnded": {
          "$className": "RemoteEvent"
        }
      },

      "GameStats": {
        "$className": "Folder",

        "TimeLeft": {
          "$className": "NumberValue"
        },

        "GameMode": {
          "$className": "StringValue"
        }
      },

      "$ignoreUnknownInstances": true
    },

    "ServerScriptService": {
      "$path": "src/ServerScriptService"
    },

    "StarterPlayer": {
      "$path": "src/StarterPlayer",

      "StarterPlayerScripts": {
        "$path": "src/StarterPlayerScripts"
      },

      "StarterCharacterScripts": {
        "$path": "src/StarterCharacterScripts"
      }
    },

    "Workspace": {
      "$properties": {
        "FilteringEnabled": true
      },
      "Baseplate": {
        "$className": "Part",
        "$properties": {
          "Anchored": true,
          "Color": [
            0.38823,
            0.37254,
            0.38823
          ],
          "Locked": true,
          "Position": [
            0,
            -10,
            0
          ],
          "Size": [
            512,
            20,
            512
          ]
        }
      }
    },
    "Lighting": {
      "$properties": {
        "Ambient": [
          0,
          0,
          0
        ],
        "Brightness": 2,
        "GlobalShadows": true,
        "Outlines": false,
        "Technology": "Voxel"
      }
    },
    "SoundService": {
      "$properties": {
        "RespectFilteringEnabled": true
      }
    }
  }
}